<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>

	<rule>
		<note>添加</note>
		<from>^/admin/index.html</from>
		<to type="forward">/admin/page!main.action</to>
	</rule>
	<rule>
		<note>项目菜单</note>
		<from>^/admin/menu_project.html</from>
		<to type="forward">/admin/menu!project.action</to>
	</rule>
	<rule>
		<note>申请管理菜单</note>
		<from>^/admin/menu_apply.html</from>
		<to type="forward">/admin/menu!apply.action</to>
	</rule>
	<rule>
		<note>会员管理菜单</note>
		<from>^/admin/menu_member.html</from>
		<to type="forward">/admin/menu!member.action</to>
	</rule>
	<rule>
		<note>管理员管理菜单</note>
		<from>^/admin/admin_manage.html</from>
		<to type="forward">/admin/menu!admin.action</to>
	</rule>

	<rule>
		<note>网站设置</note>
		<from>^/admin/menu_setting.html</from>
		<to type="forward">/admin/menu!setting.action</to>
	</rule>
	
</urlrewrite>